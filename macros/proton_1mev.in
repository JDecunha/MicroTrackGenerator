#Set verbosity
	/tracking/verbose 0
	/run/verbose 1

#geometry information
	/geometry/voxelSideLength 3 mm

#Initialize the run manager
	/run/initialize

#source information
	/gps/particle proton
	
	#beam will be oriented in -z direction by default
	/gps/ang/type beam1d 
	
	/gps/ene/type Mono
	/gps/ene/mono 1 MeV

/run/beamOn 2
